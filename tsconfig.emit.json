{
    "include": [
        "index-server.js",
        "types.d.ts",
    ],
	"compilerOptions": {
		"allowJs": true,
		"declaration": true,
		"noEmit": false,
        "emitDeclarationOnly": true,
        "stripInternal": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "outFile": "index.raw.d.ts",
        "declarationMap": true,
        "target": "ES2015",
	}
}